{
  "manifest_version": 2,
  "name": "いいね確認 for Twitter",
  "version": "1.0.0",
  "icons": {
    "16": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "content_scripts": [
    {
      "js": ["./content.js"],
      "matches": [
        "https://tweetdeck.twitter.com/*",
        "https://twitter.com/*"
      ],
      "run_at": "document_end"
    }
  ]
}
